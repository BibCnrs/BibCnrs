		"jsonwebtoken": "^9.0.2",
		"lodash.flatten": "^4.4.0",
		"lodash.flattendeep": "^4.4.0",
		"lodash.remove": "^4.7.0",
		"lodash.trim": "^4.5.1",
		"lodash.uniqby": "^4.7.0",
		"nodemailer": "^6.9.13",
		"prisma": "^5.15.0",
		"redis": "^4.6.14",
