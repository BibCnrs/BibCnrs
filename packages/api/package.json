{
	"name": "@bibcnrs/api",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build:bin": "swc bin --config-file src/bin/.swcrc --out-dir dist",
		"build": "nest build && yarn build:bin",
		"start:dev": "yarn build:bin && prisma migrate deploy && nest start --watch",
		"start:prod": "prisma migrate deploy && node dist/main",
		"start:e2e-ci": "PORT=3001; yarn run start:e2e",
		"start:e2e": "prisma migrate reset --force && tsx ./test/seed-e2e.ts && yarn run build && node dist/main",
		"test": "prisma migrate reset --force && tsx ./test/seed.ts && vitest --no-cache --watch=false packages/api",
		"test:watch": "prisma migrate reset --force && tsx ./test/seed.ts && vitest --no-cache --watch packages/api",
		"typecheck": "tsc --noEmit",
		"command:searchAlert": "node dist/bin/searchAlert.js",
		"command:searchAlert:dev": "yarn build:bin && yarn run command:searchAlert",
		"command:createAlertForTest": "node dist/bin/createAlertForTest.js",
		"command:createAlertForTest:dev": "yarn build:bin && node dist/bin/createAlertForTest.js",
		"command:cleanOldHistoryEntries": "node dist/bin/cleanOldHistoryEntries.js",
		"command:cleanOldHistoryEntries:dev": "yarn build:bin && node dist/bin/cleanOldHistoryEntries.js",
		"command:replaceInDB:dev": "yarn build:bin && node /app/packages/api/dist/bin/replacedb.js",
		"command:replaceInDB": "node /app/packages/api/dist/bin/replacedb.js"
	},
	"dependencies": {
		"@nestjs/common": "^11.0.0",
		"@nestjs/config": "^4.0.0",
		"@nestjs/core": "^11.0.0",
		"@nestjs/jwt": "^11.0.0",
		"@nestjs/mapped-types": "^2.0.5",
		"@nestjs/platform-express": "^11.0.0",
		"@prisma/client": "^6.0.0",
		"@types/multer": "^1.4.12",
		"axios": "^1.7.7",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"cookie-parser": "^1.4.6",
		"date-fns": "^3.6.0",
		"html-entities": "^2.5.2",
		"http-proxy-middleware": "^3.0.3",
		"iconv-lite": "^0.6.3",
		"jsonwebtoken": "^9.0.2",
		"lodash.flatten": "^4.4.0",
		"lodash.flattendeep": "^4.4.0",
		"lodash.remove": "^4.7.0",
		"lodash.trim": "^4.5.1",
		"lodash.uniqby": "^4.7.0",
		"nodemailer": "^6.9.15",
		"prisma": "^6.0.0",
		"redis": "^4.7.0",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"winston": "^3.14.2",
		"winston-daily-rotate-file": "^5.0.0",
		"xml-flow": "^1.0.4"
	},
	"devDependencies": {
		"@nestjs/cli": "^11.0.0",
		"@nestjs/schematics": "^11.0.0",
		"@nestjs/testing": "^11.0.0",
		"@swc/cli": "^0.7.0",
		"@swc/core": "^1.7.26",
		"@types/cookie-parser": "^1.4.7",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/lodash.flatten": "^4.4.9",
		"@types/lodash.flattendeep": "^4.4.9",
		"@types/lodash.remove": "^4.7.9",
		"@types/lodash.trim": "^4.5.9",
		"@types/lodash.uniqby": "^4.7.9",
		"@types/node": "^20.16.5",
		"@types/nodemailer": "^6.4.15",
		"@types/xml-flow": "^1.0.4",
		"@vitest/coverage-v8": "^2.0.5",
		"source-map-support": "^0.5.21",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.19.1",
		"typescript": "^5.6.2",
		"unplugin-swc": "^1.5.1",
		"vitest": "^2.0.5"
	}
}
